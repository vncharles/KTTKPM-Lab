<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chatapp.controller.Controller">
    <children>
        <ScrollPane fx:id="scrollUser" layoutX="6.0" layoutY="26.0" prefHeight="304.0" prefWidth="207.0">
            <content>
                <AnchorPane fx:id="viewUser" minHeight="0.0" minWidth="0.0" prefHeight="336.0" prefWidth="189.0" />
            </content>
        </ScrollPane>
        <ScrollPane fx:id="scrollViewChat" layoutX="213.0" layoutY="25.0" prefHeight="304.0" prefWidth="385.0">
            <content>
                <AnchorPane fx:id="anchoViewChat" minHeight="0.0" minWidth="0.0" prefHeight="333.0" prefWidth="367.0" />
            </content>
        </ScrollPane>
        <Button fx:id="btnNewChat" layoutX="69.0" layoutY="341.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="82.0" text="New User" />
        <Button fx:id="btnSend" layoutX="534.0" layoutY="347.0" mnemonicParsing="false" text="Send" />
      <TextField fx:id="txtChat" layoutX="220.0" layoutY="335.0" prefHeight="55.0" prefWidth="299.0" />
            <Label graphicTextGap="5.0" layoutX="67.0" layoutY="7.0" text="CURRENT USER" textAlignment="CENTER">
                <font>
                    <Font name="System Bold" size="12.0" />
                </font>
            </Label>
            <Label layoutX="375.0" layoutY="7.0" text="VIEW CHAT" />
    </children>
</AnchorPane>
